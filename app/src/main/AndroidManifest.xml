<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.friskybutcher.foodorder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Catagories" />
        <activity android:name=".Starters" />
        <activity android:name=".Soups" />
        <activity android:name=".Sandwiches" />
        <activity android:name=".Pasta" />
        <activity android:name=".Fish" />
        <activity android:name=".MainCourse" />
        <activity android:name=".Desert" />
        <activity android:name=".Order" />
        <activity android:name=".Pay"></activity>
    </application>

</manifest>